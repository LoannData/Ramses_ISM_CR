This namelist contains various input parameters for RAMSES runs

&RUN_PARAMS
hydro=.true.
nremap=0
ncontrol=100
nsubcycle=4*2
verbose=.false.
static_gas=.true.
/

&AMR_PARAMS 
levelmin=6
levelmax=6
ngridmax=100000
nexpand=1
boxlen=1.0
/

&RADIATION_PARAMS
/

&BOUNDARY_PARAMS
/

&INIT_PARAMS
/


&OUTPUT_PARAMS
foutput=10
noutput=1
tout=200000.
/

&HYDRO_PARAMS
gamma=1.4
courant_factor=0.1
slope_type=1
riemann='llf'
pressure_fix=.true.
/

&REFINE_PARAMS 
err_grad_d=0.01
err_grad_u=0.05
err_grad_prad=0.05
interpol_var=1
interpol_type=1
/

&PHYSICS_PARAMS
isotrope_cond=.false.
cr_diffusion=.true.
fix_temp_diff=.true.
Dcr=1.0
/

