This namelist contains various input parameters for RAMSES runs


&RUN_PARAMS
hydro=.true.
nremap=0
ncontrol=10
nsubcycle=15*2
static=.false.
verbose=.false.
fld=.false.
/

&AMR_PARAMS
levelmin=4
levelmax=6
ngridmax=20000
nexpand=10*1
boxlen=1.0
/

&INIT_PARAMS
nregion=1
region_type='square'
x_center=0.5
exp_region=10.0
length_x=1.0
d_region=1.0
u_region=0.0
p_region=1.0
A_region=0.0
B_region=0.0
C_region=0.0
/

&OUTPUT_PARAMS
noutput=2
foutput=1000000
tout=0.0,50.0
/

&HYDRO_PARAMS
gamma=1.4
courant_factor=0.7
slope_type=1
scheme='induction'
riemann='hlld'
riemann2d='hlld'
pressure_fix=.true.
/

&REFINE_PARAMS
err_grad_A=0.1
err_grad_B=0.1
err_grad_C=0.1
interpol_var=0
interpol_type=1
/

&BOUNDARY_PARAMS
/

&NONIDEALMHD_PARAMS
nambipolar=1
gammaAD=1.0
coefad=0.1d0
nminitimestep=0   ! pour la diffusion ambipolaire seulement
ntestDADM=1
/

&PHYSICS_PARAMS
mu_gas=1.0d08
/
