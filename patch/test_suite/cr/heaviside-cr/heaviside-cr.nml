This namelist contains various input parameters for RAMSES runs

&RUN_PARAMS
hydro=.true.
nremap=0
ncontrol=100
nsubcycle=2*1,2*2
verbose=.false.
static_gas=.true.
/

&AMR_PARAMS 
levelmin=3
levelmax=7
ngridmax=10000
nexpand=1
boxlen=1.0
/


&BOUNDARY_PARAMS
/

&INIT_PARAMS
nregion=2
region_type(1)='square'
region_type(2)='square'
x_center=0.5,0.5
length_x=10.0,0.5
d_region=1.0,1.0
u_region=0.0,0.0
v_region=0.0,0.0
w_region=0.0,0.0
p_region=0.8,0.8
A_region=0.0,0.0
B_region=0.0,0.0
C_region=0.0,0.0
prad_region=4,8
/


&OUTPUT_PARAMS
foutput=1
noutput=1
tout=5.d-3
/

&HYDRO_PARAMS
gamma=1.4
courant_factor=0.1
slope_type=1
riemann='llf'
pressure_fix=.true.
/

&REFINE_PARAMS 
err_grad_d=0.01
err_grad_u=0.05
err_grad_prad=0.1
interpol_var=1
interpol_type=1
/

&PHYSICS_PARAMS
cr_diffusion=.true.
isotrope_cond=.false.
Dcr=1.0
/

