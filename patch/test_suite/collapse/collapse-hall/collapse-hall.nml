!This namelist contains various input parameters for RAMSES runs

&RUN_PARAMS  
hydro=.true.
poisson=.true.
nrestart=0
ncontrol=10
nremap=2
nsubcycle=3*1,35*2
fld=.false.
radiative_nimhdheating=.false.
/

&AMR_PARAMS 
levelmin=5
levelmax=17
ngridmax=100000
npartmax=5000
nexpand=30*4
/

&BOUNDARY_PARAMS
/

&INIT_PARAMS
bb_test=.true.
mass_c=1. !in solar mass
alpha_dense_core=0.15
beta_dense_core=0.0d0
crit=0.2
delta_rho=0.1d0
theta_mag=0.0
contrast=100.d0
Mach=0.0d0
/

&OUTPUT_PARAMS
foutput=1000000
fbackup=50
noutput=2
tout=0,1.658e-04
/

&POISSON_PARAMS 
gravity_type=0
epsilon=1d-4
/

&HYDRO_PARAMS
gamma=1.666667
courant_factor=0.7
slope_type=1
scheme='muscl'
riemann='hll'
riemann2d='hll'
pressure_fix=.true.
/

&PHYSICS_PARAMS
barotrop=.true.
/

&NONIDEALMHD_PARAMS
nambipolar=1
gammaAD=3.5d13
use_x2d=1
nmagdiffu=1
etaMD=1.d0
nhall=1
coefhall=0.3
coefad=0.1d0
nminitimestep=1
coefalfven=0.3d0
coefdtohm=0.2d0
rho_threshold=4.0d+07
/

&REFINE_PARAMS 
jeans_refine=30*4.
iso_jeans=.false.
interpol_var=0
interpol_type=1
/

&RADIATION_PARAMS
grey_rad_transfer=.true.
epsilon_diff=1.d-12
fld_limiter='minerbo'
Tr_floor=10.0d0
robin=1.0
energy_fix=.true.
/
