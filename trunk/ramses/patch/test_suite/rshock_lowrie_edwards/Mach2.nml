\This namelist contains various input parameters for RAMSES runs

&RUN_PARAMS
hydro=.true.
nremap=10
ncontrol=100
nsubcycle=15*2
fld=.true.
static=.false.
verbose=.false.
/

&AMR_PARAMS
levelmin=5
levelmax=11
ngridmax=100000
nexpand=20*2
boxlen=2000.0d0
/

&INIT_PARAMS
nregion=2
region_type(1)='square'
region_type(2)='square'
x_center=500.0,1500.0
exp_region=10.0,10.0
length_x=1000,1000
d_region=5.45887d-13,1.2479d-12
u_region=2.3547d5,1.0300d5
p_region=4.5401356d-003,2.1563286d-02
/

&OUTPUT_PARAMS
noutput=2
foutput=100000000
tout=0.0,1.0d-1
/

&HYDRO_PARAMS
gamma=1.666667
courant_factor=0.5
slope_type=1
scheme='muscl'
riemann='hll'
pressure_fix=.true.
smallr=1.d-20
/

&REFINE_PARAMS
err_grad_d=0.05
err_grad_e=0.05
interpol_var=0
interpol_type=2
/

&BOUNDARY_PARAMS
nboundary = 2
ibound_min=-1,+1
ibound_max=-1,+1
bound_type= 3,3
d_bound=5.45887d-13,1.2479d-12
u_bound=2.3547d5,1.0300d5
p_bound=4.5401356d-003,2.1563286d-02
/

&RADIATION_PARAMS
grey_rad_transfer=.true.
epsilon_diff=1.d-8
fld_limiter='nolim'
robin=1.0d0
rosseland_params=0.848902d0,0.0d0,0.0d0
planck_params=3.92664d-5,0.0d0,0.0d0
energy_fix=.false.
/
