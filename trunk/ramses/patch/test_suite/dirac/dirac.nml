This namelist contains various input parameters for RAMSES runs


&RUN_PARAMS
hydro=.true.
nremap=10
ncontrol=10
nsubcycle=15*2
static=.false.
verbose=.false.
fld=.true.
/

&AMR_PARAMS
levelmin=5
levelmax=8
ngridmax=100000
nexpand=10*1
boxlen=1.0
/

&INIT_PARAMS
nregion=2
region_type(1)='square'
region_type(2)='point'
x_center=0.5,0.5
exp_region=10.0,0.
length_x=1000.,1.0
d_region=1.0,1.0
u_region=0.0,0.0
p_region=2.8187640d+11,5.01255d+12
/

&OUTPUT_PARAMS
noutput=3
foutput=1000000
tout=0.0,1.d-13,2.d-13,3d-13,2.d-12
/

&HYDRO_PARAMS
gamma=1.4
courant_factor=0.01
