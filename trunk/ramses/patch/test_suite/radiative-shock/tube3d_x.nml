\This namelist contains various input parameters for RAMSES runs

&RUN_PARAMS
hydro=.true.
nremap=10
ncontrol=10
nsubcycle=15*2
fld=.true.
static=.false.
verbose=.false.
/

&AMR_PARAMS
levelmin=4
levelmax=6
ngridmax=500000
nexpand=2
boxlen=1.0d0
/

&INIT_PARAMS
nregion=1
region_type(1)='square'
x_center=0.5
y_center=0.5
z_center=0.5
exp_region=10.0
length_x=1.0d15
length_y=1.0d15
length_z=1.0d15
d_region=1.0d0
u_region=-1.0d0
v_region=0.0d0
w_region=0.0d0
p_region=2.309d-3
/

&OUTPUT_PARAMS
noutput=2
foutput=100000
tout=0.0,3.0d-1
/

&HYDRO_PARAMS
gamma=1.4
courant_factor=0.5
slope_type=1
scheme='muscl'
riemann='hll'
pressure_fix=.true.
/

&REFINE_PARAMS
err_grad_d=0.2
err_grad_e=0.2
interpol_var=1
interpol_type=2
/

&BOUNDARY_PARAMS
nboundary = 6
ibound_min=+1,-1,-1,-1,-1,-1
ibound_max=+1,-1,+1,+1,+1,+1
jbound_min= 0, 0,-1,+1,-1,-1
jbound_max= 0, 0,-1,+1,+1,+1
kbound_min= 0, 0, 0, 0,-1,+1
kbound_max= 0, 0, 0, 0,-1,+1
bound_type= 3,1,0,0,0,0
d_bound=1.0d0
u_bound=-1.0d0
v_bound=0.0d0
w_bound=0.0d0
p_bound=2.309d-3
/

&RADIATION_PARAMS
grey_rad_transfer=.true.
rosseland_params=3.1d-10,-1.0,0.0
planck_params=3.1d-10,-1.0,0.0
epsilon_diff=1.d-8
fld_limiter='minerbo'
numax=1.5d14
extra_end_group=.true.
/

&PHYSICS_PARAMS
units_density=7.78d-10
units_time=1.16666667e5
units_length=7.0d10
/
